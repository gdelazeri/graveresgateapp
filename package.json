{
  "name": "graveresgateapp",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "prettify": "prettier --write \"**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-picker/picker": "2.4.10",
    "@react-native-segmented-control/segmented-control": "2.4.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "add": "^2.0.6",
    "axios": "^1.6.2",
    "expo": "~49.0.0",
    "expo-font": "~11.4.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "fuse.js": "^7.0.0",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-native-calendars": "1.1268.0",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mask-text": "^0.14.2",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-paper": "^5.11.1",
    "react-native-picker-select": "^9.0.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-text-input-mask": "^3.2.0",
    "react-native-touchable-scale": "^2.2.0",
    "styled-components": "^6.1.1",
    "xdate": "^0.8.2",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.4.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.8",
    "@types/moment": "^2.13.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "^0.72.2",
    "@types/react-native-text-input-mask": "^0.7.9",
    "@types/styled-components-react-native": "^5.2.5",
    "@types/xdate": "^0.8.35",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "prettier": "^3.1.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "moduleNameMapper": {
      "@assets/(.*)": "<rootDir>/assets/$1",
      "@api/(.*)": "<rootDir>/src/api/$1",
      "@context/(.*)": "<rootDir>/src/context/$1",
      "@routes/(.*)": "<rootDir>/src/routes/$1",
      "@screens/(.*)": "<rootDir>/src/screens/$1",
      "@theme/(.*)": "<rootDir>/src/theme/$1",
      "@utils/(.*)": "<rootDir>/src/utils/$1"
    },
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "private": true
}
